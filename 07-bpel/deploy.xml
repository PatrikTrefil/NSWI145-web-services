<?xml version="1.0" encoding="utf-8"?>
<deploy
    xmlns="http://www.apache.org/ode/schemas/dd/2007/03"
    xmlns:tnsbpel="http://nswi145/Reservation/bpel"
    xmlns:tnswsdl="http://nswi145/Reservation/wsdl"
    xmlns:services="http://soap.patriktrefil.com/"
>

    <process name="tnsbpel:Reservation">
        <active>true</active>
        <provide partnerLink="ReservationPartnerLink">
            <service name="tnswsdl:ReservationService" port="ReservationPort" />
        </provide>
        <invoke partnerLink="EmailPartnerLink">
            <service name="services:EmailImplService" port="EmailImplPort" />
        </invoke>
        <provide partnerLink="EmailPartnerLink">
            <service name="tnswsdl:ReservationService" port="EmailPort" />
        </provide>
        <invoke partnerLink="BookingPartnerLink">
            <service name="services:BookingImplService" port="BookingImplPort" />
        </invoke>
        <provide partnerLink="BookingPartnerLink">
            <service name="tnswsdl:ReservationService" port="BookingPort" />
        </provide>
    </process>
</deploy>